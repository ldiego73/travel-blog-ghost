{
  "name": "travel",
  "description": "A clean, minimal default theme for the Ghost publishing platform",
  "demo": "https://www.chapandomochila.com/",
  "version": "1.0.0",
  "engines": {
    "ghost": ">=3.0.0",
    "ghost-api": "v3"
  },
  "author": {
    "name": "Luis Diego",
    "email": "lfdiego7@gmail.com"
  },
  "gpm": {
    "type": "theme",
    "categories": [
      "Clean",
      "Minimal",
      "Responsive",
      "Travel"
    ]
  },
  "keywords": [
    "ghost",
    "theme",
    "ghost-theme"
  ],
  "config": {
    "posts_per_page": 7,
    "image_sizes": {
      "xxs": {
        "width": 30
      },
      "xs": {
        "width": 100
      },
      "s": {
        "width": 300
      },
      "m": {
        "width": 600
      },
      "l": {
        "width": 1000
      },
      "xl": {
        "width": 2000
      }
    }
  },
  "devDependencies": {
    "@tryghost/content-api": "^1.4.0",
    "bulma": "^0.8.1",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "fuse.js": "^5.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "develop": "webpack --mode development --watch",
    "build:develop": "webpack --mode development",
    "build:production": "webpack --mode production"
  },
  "dependencies": {
    "custom-env": "^2.0.1"
  }
}
